{
  "name": "awsmug/torro-forms-plugin-boilerplate",
  "description": "Plugin Boilerplate for Torro Forms.",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "extension",
    "torro forms",
    "forms",
    "form builder",
    "api"
  ],
  "homepage": "https://plugin-website.com",
  "authors": [
    {
      "name": "Sven Wagener",
      "email": "contact@awesome.ug",
      "homepage": "http://www.awesome.ug",
      "role": "Product Lead"
    },
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "homepage": "https://leaves-and-love.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/awsmug/torro-forms-plugin-boilerplate/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.8.20 <6.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "0.14.*",
    "wimg/php-compatibility": "^8"
  },
  "scripts":     {
    "post-install-cmd": [
      "[[ \"$COMPOSER_DEV_MODE\" == \"1\" ]] && vendor/bin/phpcs --config-set installed_paths ../../wimg/php-compatibility,../../wp-coding-standards/wpcs"
    ],
    "post-update-cmd": [
      "[[ \"$COMPOSER_DEV_MODE\" == \"1\" ]] && vendor/bin/phpcs --config-set installed_paths ../../wimg/php-compatibility,../../wp-coding-standards/wpcs"
    ]
  }
}
