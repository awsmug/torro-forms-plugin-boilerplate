{
  "name": "awsmug/torro-forms-plugin-boilerplate",
  "description": "Plugin Boilerplate for Torro Forms.",
  "version": "1.0.0",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "extension",
    "torro forms",
    "forms",
    "form builder",
    "api"
  ],
  "homepage": "https://plugin-website.com",
  "authors": [
    {
      "name": "Sven Wagener",
      "email": "contact@awesome.ug",
      "homepage": "http://www.awesome.ug",
      "role": "Product Lead"
    },
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "homepage": "https://leaves-and-love.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/awsmug/torro-forms-plugin-boilerplate/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0",
    "api-api/structure-wordpress": "1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.8.20 <6.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "^1",
    "wimg/php-compatibility": "^8",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
  },
  "scripts":     {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
